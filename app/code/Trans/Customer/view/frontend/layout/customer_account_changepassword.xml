<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="columns.top">
            <block class="Magento\Framework\View\Element\Template" name="custom.title.myaccount" before="-" template="Magento_Customer::title-myaccount.phtml">
                <action method="setPageTitle">
                    <argument name="name" xsi:type="string">My Account</argument>
                </action>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Trans\Customer\Block\Account\Form\ChangePassword" name="changePassword" template="Trans_Customer::form/changepassword.phtml" />
        </referenceContainer>
    </body>
</page>
