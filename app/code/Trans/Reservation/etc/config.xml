                <multi_store>1</multi_store>
                <enable_max_qty>0</enable_max_qty>
                <default_qty>1</default_qty>
                <buffer>2</buffer>
                <max_item_qty>2</max_item_qty>
                <priority>{"_1566791463565_565":{"sort":"1","filter":"Store"},"_1566791474835_835":{"sort":"2","filter":"Product"},"_1566791480491_491":{"sort":"3","filter":"Category"}}</priority>
