<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="columns.top">
            <container name="category.view.title.container" htmlTag="div" htmlClass="page-blockTitle category-view-title" before="-">
                
            </container>
        </referenceContainer>

        <move element="page.main.title" destination="category.view.title.container" before="-" />
        <move element="category.view.container" destination="category.view.title.container" after="page.main.title" />
    </body>
</page>
